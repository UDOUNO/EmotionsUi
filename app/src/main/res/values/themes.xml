<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.NoActionBar" parent="Theme.Material3.Dark.NoActionBar" >
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="windowActionBarOverlay">true</item>
        <item name="colorSurface">@color/ios_thumb_off</item>
    </style>
    <style name="Theme.EmotionsApp.TimepickerDialog" parent="ThemeOverlay.Material3.MaterialTimePicker">
        <item name="android:fontFamily">@font/velasans_regular</item>
        <item name="android:textAllCaps">false</item>
        <item name="colorPrimary">@color/white</item>
        <item name="colorOnSurface">@color/white</item>
        <item name="colorSurfaceContainerHigh">@color/dark_gray</item>
        <item name="colorOnSurfaceVariant">@color/white</item>
        <item name="colorSurfaceContainerHighest">@color/dark_gray</item>
        <item name="colorPrimaryContainer">@color/white</item>
    </style>
